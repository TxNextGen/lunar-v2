{
  "name": "lunar-v2",
  "type": "module",
  "version": "2.0.0",
  "scripts": {
    "dev": "npx astro dev --host 0.0.0.0",
    "start": "node --trace-warnings --enable-source-maps --import tsx index.ts",
    "build": "astro telemetry disable && npx astro check && npx astro build",
